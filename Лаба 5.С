#include <stdio.h>

int main() {
    int i, j;

    // Вивід квадрата Піфагора
    printf("Квадрат Піфагора:\n");

    for (i = 1; i <= 10; i++) {
        for (j = 1; j <= 10; j++) {
            printf("%4d ", i * j);
        }
        printf("\n");
    }

    // Вивід таблиці множення
    printf("\nТаблиця множення:\n");

    for (i = 1; i <= 10; i++) {
        for (j = 1; j <= 10; j++) {
            printf("%4d ", i * j);
        }
        printf("\n");
    }

    return 0;
}
