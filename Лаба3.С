#include <stdio.h> 
 
int main() { 
    int year; 
 
    printf("Введіть рік: "); 
    scanf("%d", &year); 
 
    if ((year % 4 == 0 && year % 100 != 0) || year % 400 == 0) { 
        printf("%d рік є високосним.", year); 
    } else { 
        printf("%d рік не є високосним.", year); 
    } 
 
    return 0; 
}
